{
  "name": "kevco-portfolio",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "prerender": "react-snap",
    "preview": "vite preview"
  },
  "reactSnap": {
    "source": "dist",
    "include": ["/", "/skills", "/projects", "/demos", "/contact"],
    "minifyHtml": { "collapseWhitespace": false, "removeComments": false },
    "puppeteerArgs": ["--no-sandbox", "--disable-setuid-sandbox"],
    "skipThirdPartyRequests": true
  },
  "dependencies": {
    "@iconify/react": "^4.1.1",
    "gsap": "^3.12.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^6.22.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "postcss": "^8.4.35",
    "react-snap": "^1.23.0",
    "tailwindcss": "^3.4.1",
    "vite": "^5.1.0"
  }
}
